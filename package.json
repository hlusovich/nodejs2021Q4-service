{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "description": "REST service starter for RS School",
  "main": "srcOld/server.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "lint": "eslint --ext .ts --fix ./",
    "docker": "cross-env POSTGRES_HOST='postgres' ts-node   srcOld/server.ts",
    "start:dev": "nest start --watch",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ormconfig.ts",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:generate": "npm run typeorm:cli -- migration:create -n MyMigration"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "hapi",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@babel/types": "^7.16.8",
    "@hapi/hapi": "^20.2.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@typeform/eslint-config": "^3.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/uuid": "^8.3.3",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "dotenv": "10.0.0",
    "es-dirname": "^0.1.0",
    "expose": "^0.1.4",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-node": "^10.4.0",
    "typeorm": "^0.2.41",
    "typeorm-extension": "^1.0.3",
    "uuid": "8.3.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.11.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chai": "4.3.4",
    "debug": "4.3.2",
    "dirty-chai": "2.0.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-typeorm": "^0.0.19",
    "husky": "^4.3.8",
    "jest": "27.3.1",
    "lint-staged": "12.0.3",
    "nodemon": "2.0.15",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
